<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h1>Hello from server</h1>
    <ul>
        <% urls.forEach(function(url) { %>
            <div>
                <strong>
                    <%= url.shortid %>
                </strong>
                <%=url.redirect_url%>
                    <div>
                        <%=url.visitorHistory.length%>
                    </div>
            </div>
            <% }); %>
    </ul>
</body>

</html>